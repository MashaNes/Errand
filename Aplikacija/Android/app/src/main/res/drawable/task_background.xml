<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape
			android:shape="rectangle">
			<solid
				android:color="@color/colorLightGrey"/>
		</shape>
	</item>

	<item
		android:left="14dp"
		android:top="32dp"
